            Grid.Row="1"
            AutoGenerateColumns="False"
            ItemsSource="{Binding Devices}">
            <DataGrid.Columns>
                <DataGridTextColumn
                    Width="150"
                    Binding="{Binding DeviceId}"
                    Header="DeviceId" />
